<div class="container">
  <h2>管理者ログイン</h2>

  <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
    <div class="form_group">
      <%= f.label :メールアドレス,class:"col-sm-3" %>
      <%= f.email_field :email, autofocus: true, autocomplete: "email",class:"col-sm-3" %>
    </div>

    <div class="form_group">
      <%= f.label :パスワード,class:"col-sm-3" %>
      <%= f.password_field :password, autocomplete: "current-password",class:"col-sm-3" %>
    </div>

    <div class="actions">
      <div class="col-sm-3"> </div>
      <%= f.submit "Log in",class:"btn btn-primary" %>
    </div>
  <% end %>
</div>

<%= render "admin/shared/links" %>
